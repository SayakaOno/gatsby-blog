    "invariant": "^2.2.4",
    "katex": "^0.10.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.9.7",
    "node-sass": "^4.12.0",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.8.6",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
